{
    "name": "maximaster/cli-ent",
    "type": "library",
    "description": "Guzzle handler to imitate HTTP calls through CLI",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Maximaster\\CliEnt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maximaster\\CliEnt\\Test\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.6",
        "guzzlehttp/promises": "^1.3",
        "symfony/process": "^5.0",
        "guzzle/parser": "^3.9",
        "guzzlehttp/guzzle": "^6.2 || ^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phan/phan": "^3.0"
    },
    "suggest": {
        "ext-runkit": "You should use runkit or runkit7",
        "ext-runkit7": "You should use runkit or runkit7"
    },
    "scripts": {
        "test": "phpunit"
    }
}
