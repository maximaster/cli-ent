{
  "name": "maximaster/cli-ent",
  "type": "library",
  "description": "Guzzle handler to imitate HTTP calls through CLI",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Maximaster\\CliEnt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maximaster\\CliEnt\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "symfony/process": "^7.0",
    "guzzlehttp/psr7": "^1.6",
    "guzzlehttp/promises": "^1.3",
    "guzzle/parser": "^3.9",
    "guzzlehttp/guzzle": "^6.2 || ^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phan/phan": "^5.4"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phan"
  }
}
